{
  "name": "@ndct/rxjs-operators",
  "version": "1.0.0",
  "description": "Rxjs操作符扩展",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "npm run clean && tsc -b ./tsconfig.json ./tsconfig.cjs.json",
    "clean": "rimraf -r ./dist",
    "lint": "prettier */**/*.ts --write && eslint */**/*.ts --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/rxjs-operators.git"
  },
  "keywords": [
    "rxjs",
    "rxjs-operators"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NoDocCat/rxjs-operators/issues"
  },
  "homepage": "https://github.com/NoDocCat/rxjs-operators#readme",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "peerDependencies": {
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
