{
  "name": "@ndct/rxjs-operators",
  "version": "1.1.1",
  "description": "Rxjs 操作符扩展",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run compile",
    "compile": "tsc -b ./tsconfig.json ./tsconfig.cjs.json",
    "clean": "rimraf -r ./dist",
    "lint": "prettier --check \"*/**/*.ts\" && eslint \"*/**/*.ts\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/rxjs-operators.git"
  },
  "keywords": [
    "rxjs",
    "rxjs-operators"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NoDocCat/rxjs-operators/issues"
  },
  "homepage": "https://github.com/NoDocCat/rxjs-operators#readme",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "peerDependencies": {
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
