{
  "name": "@ndct/rxjs-operators",
  "version": "1.3.0",
  "description": "Rxjs 操作符扩展",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rollup -c --failAfterWarnings",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier --check {**/*,*}.{js,ts,json,yml,md} --ignore-path ./.gitignore",
    "lint:eslint": "eslint {**/*,*}.{js,ts} --ignore-path ./.gitignore",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/rxjs-operators.git"
  },
  "keywords": [
    "rxjs",
    "rxjs-operators"
  ],
  "author": {
    "name": "NoDocCat",
    "email": "nodoccat@outlook.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NoDocCat/rxjs-operators/issues"
  },
  "homepage": "https://github.com/NoDocCat/rxjs-operators#readme",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rollup": "^2.38.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rxjs": "^6.6.3",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
