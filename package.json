{
  "name": "@ndct/rxjs-operators",
  "version": "1.2.0",
  "description": "Rxjs 操作符扩展",
  "scripts": {
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:prettier": "prettier --check {**/*,*}.{js,ts,json}",
    "lint:eslint": "eslint {**/*,*}.{js,ts}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/rxjs-operators.git"
  },
  "keywords": [
    "rxjs",
    "rxjs-operators"
  ],
  "author": {
    "name": "NoDocCat",
    "email": "nodoccat@outlook.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NoDocCat/rxjs-operators/issues"
  },
  "homepage": "https://github.com/NoDocCat/rxjs-operators#readme",
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run lint"
    }
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  }
}
